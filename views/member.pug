            .input-group-append
              button.btn.btn-outline-secondary Thêm 

        script.
          $( function() {
            $( "#add" ).autocomplete({
              source: function(req,res){
                $.ajax({
                  url: "/search/allUser",
                  dataType: "jsonp",
                  type: "GET",
                  data:req,
                  success: function(data){
                    res(data)
                  },
                  eror: function(err){
                    console.log(err.status);
                  }
                });
              },
              minLength:1,
              select: function(event,ui){
                if(ui.item){
                  $('#add').val(ui.item.label);
                }
              }


            });
          });

        
        if members
          each _member in members
            p=_member.displayName 